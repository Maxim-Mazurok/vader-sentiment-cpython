{
  "name": "vader-sentiment-cpython",
  "version": "0.0.1",
  "scripts": {
    "install": "node-gyp-build",
    "start": "bash ./start.sh",
    "test": "npm run prebuild && npm run run-test",
    "run-test": "node test.js",
    "run-test-mac": "PYTHONHOME=/usr/local/opt/python/Frameworks/Python.framework/Versions/Current node test.js",
    "prebuild": "prebuildify --napi --strip"
  },
  "dependencies": {
    "node-addon-api": "^3.1.0",
    "node-gyp-build": "^4.2.3"
  },
  "devDependencies": {
    "node-gyp": "^8.0.0",
    "prebuildify": "^4.1.2"
  }
}
